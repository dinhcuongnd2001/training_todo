datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
}

model Todo {
  id Int @id @default(autoincrement())
  name String @unique
  score String 
  desc String 
  status String
}